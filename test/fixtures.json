[
  {
    "name": "Unknown libraries render as libraries",
    "tests": [
      {
        "input": "boost:a[]",
        "output": "https://www.boost.org/libs/a[Boost.A]"
      }
    ]
  },
  {
    "name": "Links to Libraries",
    "tests": [
      {
        "input": "boost:core[]",
        "output": "https://www.boost.org/libs/core[Boost.Core]"
      }
    ]
  },
  {
    "name": "Tools have different link formats",
    "tests": [
      {
        "input": "boost:docca[]",
        "output": "https://www.boost.org/tools/docca[Boost.Docca]"
      }
    ]
  },
  {
    "name": "Positional arguments",
    "tests": [
      {
        "input": "boost:boostdep[Dependencies]",
        "output": "https://www.boost.org/tools/boostdep[Dependencies]"
      }
    ]
  },
  {
    "name": "Forward attributes",
    "tests": [
      {
        "input": "boost:quickbook[Quickbook,window=_blank]",
        "output": "https://www.boost.org/tools/quickbook[Quickbook,window=_blank]"
      },
      {
        "input": "boost:utility[window=_blank]",
        "output": "https://www.boost.org/libs/utility[Boost.Utility,window=_blank]"
      }
    ]
  },
  {
    "name": "Exceptions to Pascal Case",
    "tests": [
      {
        "input": "boost:url[]",
        "output": "https://www.boost.org/libs/url[Boost.URL]"
      },
      {
        "input": "boost:build[]",
        "output": "https://www.boost.org/tools/build[B2]"
      }
    ]
  }
]